{
  "manifest_version": 3,
  "name": "web-helper",
  "version": "1.0.0",
  "description": "Productivity tools for web browsing",
  "permissions": [&#34;storage&#34;,&#34;tabs&#34;,&#34;bookmarks&#34;],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
