{
  "manifest_version": 3,
  "name": "<%= name %>",
  "version": "<%= version %>",
  "description": "<%= description %>",
  "permissions": <%= JSON.stringify(permissions) %>,
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  }
}
