{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es6", "dom"],
        "moduleResolution": "node",
        "typeRoots": ["./node_modules/@types", "./node_modules/@figma"],
        "jsx": "react",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist"
    }
}
