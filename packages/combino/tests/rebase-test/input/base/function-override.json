{
	"defaultPath": "<%= rebase('src') %>",
	"cwdPath": "<%= rebase('src', {format: 'cwd'}) %>",
	"relativePath": "<%= rebase('src', {format: 'relative'}) %>",
	"absolutePath": "<%= rebase('src', {format: 'absolute'}) %>",
	"nestedCwd": "<%= rebase('src/components', {format: 'cwd'}) %>",
	"nestedRelative": "<%= rebase('src/components', {format: 'relative'}) %>",
	"nestedAbsolute": "<%= rebase('src/components', {format: 'absolute'}) %>"
}
